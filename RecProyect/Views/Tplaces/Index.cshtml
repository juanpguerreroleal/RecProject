
@{
    ViewData["Title"] = "Index";
}


<div class="row table-bordered" style="padding: 10px;">
    <h2 class="col-lg-12">Top 10 Places that match with your profile</h2>
    <div class="row">
        <div class="col-md-4 col-sm-12"><h3>Name</h3></div>
        <div class="col-md-8 col-sm-12"><h3>Description</h3></div>
    </div>
    <div class="row">
        <div class="col-md-4 col-sm-12">
            <h4>Name of the place</h4>
        </div>
        <div class="col-md-8 col-sm-12">
            <h4>Description of the place</h4>

        </div>
    </div>
</div>
<div class="row table-bordered">
    <aside class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
        <h4>Add visited place</h4>
        <input class="btn btn-block btn-sm btn-primary" value="Add" />
    </aside>
    <article class="col-lg-8 col-md-8 col-sm-8 col-xs-12 table-bordered">
        <h3>Places that i'd visited</h3>
        <div class="row">
            <div class="col-md-4 col-sm-12"><h4>Name Ex</h4></div>
            <div class="col-md-5 col-sm-12"><h4>Description Ex</h4></div>
            <div class="col-md-3 col-sm-12 btn-group">
                <input class="btn btn-danger btn-sm" value="Delete" />
                <input class="btn btn-primary btn-sm" value="Edit" />
            </div>
        </div>
    </article>
</div>